    "definitions": {
        "domain.PriceHistory": {
            "type": "object",
            "properties": {
                "month": {
                    "type": "string",
                    "example": "abril de 2024"
                },
                "price": {
                    "type": "string",
                    "example": "R$ 10.000,00"
                },
                "reference": {
                    "type": "string",
                    "example": "308"
                }
            }
        },
        "domain.Reference": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "example": "308"
                },
                "month": {
                    "type": "string",
                    "example": "abril de 2024"
                }
            }
        },
        "domain.VehicleBrand": {
            "description": "Brand of a vehicle",
            "type": "object",
            "properties": {
                "code": {
                    "description": "Brand code",
                    "type": "string",
                    "example": "23"
                },
                "name": {
                    "description": "Brand name",
                    "type": "string",
                    "example": "VW - VolksWagen"
                }
            }
        },
        "domain.VehicleDetail": {
            "description": "Vehicle details",
            "type": "object",
            "properties": {
                "brand": {
                    "description": "Brand of the vehicle",
                    "type": "string",
                    "example": "VW - VolksWagen"
                },
                "codeFipe": {
                    "description": "Unique Fipe identifier",
                    "type": "string",
                    "example": "005340-6"
                },
                "fuel": {
                    "description": "Fuel used by the vehicle",
                    "type": "string",
                    "example": "Diesel"
                },
                "fuelAcronym": {
                    "description": "Fuel acronym (eg. G)",
                    "type": "string",
                    "example": "D"
                },
                "model": {
                    "description": "Model of the vehicle",
                    "type": "string",
                    "example": "AMAROK High.CD 2.0 16V TDI 4x4 Dies. Aut"
                },
                "modelYear": {
                    "description": "Year of vehicle production",
                    "type": "integer",
                    "example": 2014
                },
                "price": {
                    "description": "Price in Brazilian Real",
                    "type": "string",
                    "example": "R$ 10.000,00"
                },
                "priceHistory": {
                    "description": "Price history",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/domain.PriceHistory"
                    }
                },
                "referenceMonth": {
                    "description": "Month of the price",
                    "type": "string",
                    "example": "abril de 2024"
                },
                "vehicleType": {
                    "description": "Vehicle type code",
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "domain.VehicleModel": {
            "description": "Model of a vehicle",
            "type": "object",
            "properties": {
                "code": {
                    "description": "Model code",
                    "type": "string",
                    "example": "5585"
                },
                "name": {
                    "description": "Model name",
                    "type": "string",
                    "example": "AMAROK CD2.0 16V/S CD2.0 16V TDI 4x2 Die"
                }
            }
        },
        "domain.VehicleYear": {
            "description": "Year of a vehicle",
            "type": "object",
            "properties": {
                "code": {
                    "description": "Year code",
                    "type": "string",
                    "example": "2022-3"
                },
                "name": {
                    "description": "Year name",
                    "type": "string",
                    "example": "2022 Diesel"
                }
            }
        }
    },
